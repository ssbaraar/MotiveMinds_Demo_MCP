# SAP System Configuration
# Example: https://your-sap-host:your-port/sap/opu/odata/sap/API_BUSINESS_PARTNER/?$format=xml

# SAP Host (without port)
SAP_HOST=https://your-sap-host

# SAP Port
SAP_PORT=your-port

# SAP Client (get this from your SAP admin)
SAP_CLIENT=your-client

# Authentication credentials (replace with actual values)
AUTH_USERNAME="your-username"
AUTH_PASSWORD="your-password"

# # ngrok Configuration (optional - get from ngrok dashboard)
# NGROK_AUTH_TOKEN=your-ngrok-auth-token

# API Security (optional - for production)
API_KEY=your-api-key

# Logging Configuration
LOG_LEVEL=INFO

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=False

ngrok_url=https://f29d7a86168d.ngrok-free.app # Example ngrok URL (replace with your actual ngrok URL)